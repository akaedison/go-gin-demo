# login
curl -v POST \
  http://localhost:8000/base/login \
  -H 'content-type: application/json' \
  -d '{ "username": "zwz", "password": "123456" }'

# register
curl -v POST \
  http://localhost:8000/user/register \
  -H 'content-type: application/json' \
  -d '{ "username": "zwz", "password": "123456" }'

